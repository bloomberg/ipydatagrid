{
  "name": "jupyter-datagrid",
  "version": "0.1.0",
  "description": "Fast Datagrid widget for the Jupyter Notebook and JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "lib/**/*.js.map",
    "lib/**/*.ts",
    "dist/*.js",
    "dist/*.js.map",
    "dist/*.png",
    "style/**/*.*"
  ],
  "homepage": "https://github.com/QuantStack/ipydatagrid",
  "bugs": {
    "url": "https://github.com/QuantStack/ipydatagrid/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "QuantStack",
    "email": ""
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantStack/ipydatagrid"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension",
    "build:labextension": "npm run clean:labextension && mkdirp datagrid/labextension && cd datagrid/labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:nbextension": "webpack -p",
    "build:widget-examples": "cd widget-examples/basic && webpack -p",
    "build:all": "npm run build:labextension && npm run build:nbextension && npm run build:widget-examples",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf datagrid/labextension",
    "clean:nbextension": "rimraf datagrid/nbextension/static/index.js",
    "lint": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "prepack": "npm run build:lib",
    "test": "jest",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2 || ^3",
    "@jupyterlab/apputils": "^2.1.1",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/commands": "^1.10.1",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/datagrid": "^0.6.0",
    "@lumino/default-theme": "^0.2.4",
    "@lumino/domutils": "^1.1.7",
    "@lumino/messaging": "^1.3.3",
    "@lumino/virtualdom": "^1.6.1",
    "@lumino/widgets": "^1.11.1",
    "bqplot": "^0.4.6",
    "d3-array": "^2.2.0",
    "d3-color": "^1.3.0",
    "d3-format": "^1.3.2",
    "d3-scale": "^3.0.0",
    "d3-time-format": "^2.1.3",
    "moment": "^2.24.0",
    "vega-expression": "^2.6.0",
    "vega-functions": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@jupyter-widgets/jupyterlab-manager": "^2.0.0",
    "@lumino/application": "^1.6.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.11.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "acorn": "^6.2.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^3.0.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^4.0.0",
    "fs-extra": "^7.0.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.11",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svg-url-loader": "~3.0.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "~3.6.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
