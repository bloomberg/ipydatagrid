{
  "name": "jupyter-datagrid",
  "version": "0.1.0",
  "description": "Fast Datagrid widget for the Jupyter Notebook and JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/QuantStack/ipydatagrid",
  "bugs": {
    "url": "https://github.com/QuantStack/ipydatagrid/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "QuantStack",
    "email": ""
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantStack/ipydatagrid"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension",
    "build:labextension": "npm run clean:labextension && mkdirp datagrid/labextension && cd datagrid/labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:nbextension": "webpack -p",
    "build:all": "npm run build:labextension && npm run build:nbextension",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf datagrid/labextension",
    "clean:nbextension": "rimraf datagrid/nbextension/static/index.js",
    "prepack": "npm run build:lib",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@phosphor/algorithm": "^1.1.3",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/datagrid": "^0.1.10",
    "@phosphor/default-theme": "^0.1.4",
    "@phosphor/widgets": "^1.6.0",
    "bqplot": "^0.4.6",
    "d3-array": "^2.2.0",
    "d3-format": "^1.3.2",
    "d3-scale": "^3.0.0",
    "d3-time-format": "^2.1.3",
    "vega-expression": "^2.6.0",
    "vega-functions": "^5.3.0"
  },
  "devDependencies": {
    "@phosphor/application": "^1.6.0",
    "@types/node": "^10.11.6",
    "@types/webpack-env": "^1.13.6",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^7.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "acorn": "^6.2.0"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
